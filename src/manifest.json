{
  "manifest_version": 3,
  "name": "Sherlock Wallet",
  "description": "",
  "version": "0.0.1",
  "icons": {
    "16": "resources/icon16.png",
    "32": "resources/icon32.png",
    "48": "resources/icon48.png",
    "128": "resources/icon128.png"
  },
  "action": {
    "default_popup": "ui/popup.html"
  },
  "background": {
    "service_worker": "background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content/index.ts"
      ]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "chromeos": "Alt+Shift+X",
        "linux": "Alt+Shift+X",
        "mac": "Alt+Shift+X",
        "windows": "Alt+Shift+X"
      }
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
  ]
}